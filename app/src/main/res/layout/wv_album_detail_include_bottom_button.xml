<?xml version="1.0" encoding="utf-8"?>
<merge
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/wv_album_detail_btn_super"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="10dp"
        android:background="@drawable/video_item_button_bg"
        android:text="@string/btn_phone_super"
        android:textColor="@color/grey_medium"
        android:visibility="gone"/>

    <Button
        android:id="@+id/wv_album_detail_btn_normal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:background="@drawable/video_item_button_bg"
        android:text="@string/btn_phone_normal"
        android:textColor="@color/grey_medium"
        android:visibility="gone"/>

    <Button
        android:id="@+id/wv_album_detail_btn_high"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:background="@drawable/video_item_button_bg"
        android:text="@string/btn_phone_high"
        android:textColor="@color/grey_medium"
        android:visibility="gone"/>
</merge>